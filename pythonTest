def solve(eq,var='x'):
  eq1 = eq.replace("=","-(")+")"
  c = eval(eq1,{var:1j})
  return -c.real/c.imag

str = "1"
while(str != "0"):
    str = input("Input Formula: ")
    print("The Input Formula is : %s" %str)
    str.replace('โก','x')
    str.replace('รท','/')
    print("The Transfered Formula is : %s" %str)
    print(solve(str))
